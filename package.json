{
    "name": "chg-gestor-documental-front-end",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "prestart": "npm run lint && npm run build --env=%npm_config_env%",
        "start": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --configuration %npm_config_env%",
        "start:local": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --configuration %npm_config_env%",
        "watch": "ng build --watch --configuration %npm_config_env%",
        "test": "ng test",
        "test:coverage": "ng test --watch --code-coverage",
        "lint": "eslint . tsconfig.json --fix",
        "lint-fix": "eslint . --fix",
        "format": "prettier --write ."
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "~13.0.3",
        "@angular/cli": "~13.0.3",
        "@angular/compiler-cli": "~13.0.0",
        "@angular/animations": "~13.0.0",
        "@angular/cdk": "^13.0.2",
        "@angular/common": "~13.0.0",
        "@angular/compiler": "~13.0.0",
        "@angular/core": "~13.0.0",
        "@angular/forms": "~13.0.0",
        "@angular/material": "^13.0.2",
        "@angular/platform-browser": "~13.0.0",
        "@angular/platform-browser-dynamic": "~13.0.0",
        "@types/node": "^12.11.1",
        "@angular/router": "~13.0.0",
        "@ngrx/effects": "^13.0.1",
        "@ngrx/store": "^13.0.1",
        "@ngrx/store-devtools": "^13.0.1",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "bootstrap": "^5.1.3",
        "flag-icon-css": "^4.1.6",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@types/jasmine": "~3.10.0",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine-core": "~3.10.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "prettier": "^2.4.1",
        "redux-devtools-extension": "^2.13.9",
        "typescript": "~4.4.3"
    }
}

{
    "name": "chg-gestor-documental-front-end",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "prestart": "npm run lint && npm run build --env=%npm_config_env%",
        "start": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --configuration %npm_config_env%",
        "start:local": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --configuration %npm_config_env%",
        "watch": "ng build --watch --configuration %npm_config_env%",
        "test": "ng test",
        "test:coverage": "ng test --watch=true --code-coverage",
        "dockerize": "docker-compose -f docker-compose.yml -f docker/docker-compose.%npm_config_env%.yml  --profile %npm_config_service% up --build -d",
        "lint": "eslint . tsconfig.json --fix",
        "lint-fix": "eslint . --fix",
        "format": "prettier --write .",
        "sonar": "sonar-scanner"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.2.12",
        "@angular/cdk": "^13.0.2",
        "@angular/common": "^14.2.12",
        "@angular/compiler": "^14.2.12",
        "@angular/core": "^14.2.12",
        "@angular/forms": "^14.2.12",
        "@angular/material": "^13.0.2",
        "@angular/platform-browser": "^14.2.12",
        "@angular/platform-browser-dynamic": "^14.2.12",
        "@angular/router": "^14.2.12",
        "@ngrx/effects": "^14.3.2",
        "@ngrx/store": "^14.3.2",
        "@ngrx/store-devtools": "^14.3.2",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "@types/node": "^18.11.9",
        "bootstrap": "^5.1.3",
        "flag-icons": "^6.6.6",
        "rxjs": "~7.5.7",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.2.10",
        "@angular/cli": "^14.2.10",
        "@angular/compiler-cli": "^14.2.12",
        "@redux-devtools/extension": "^3.2.3",
        "@types/jasmine": "~4.3.0",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.1",
        "karma-chrome-launcher": "~3.1.1",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "prettier": "^2.8.0",
        "typescript": "~4.8.4",
        "sonarqube-scanner": "^2.9.1"
    }
}
